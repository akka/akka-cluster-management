{
  "id": "/nginx-container-multinetwork",
  "cpus": 0.1,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "labels": {
    "ACTOR_SYSTEM_NAME": "test-system-2"
  },
  "container": {
    "type": "MESOS",
    "docker": {
      "forcePullImage": false,
      "image": "nginx:1.15.2",
      "privileged": false
    },
    "portMappings": [
      {
        "containerPort": 80,
        "name": "http",
        "networkNames": ["dcos"]
      },
      {
        "containerPort": 443,
        "name": "akkamgmthttp",
        "networkNames": ["testcn"]
      }
    ]
  },
  "networks": [
    {
      "mode": "container",
      "name": "dcos"
    },
   {
      "mode": "container",
      "name": "testcn"
    }
  ]
}
